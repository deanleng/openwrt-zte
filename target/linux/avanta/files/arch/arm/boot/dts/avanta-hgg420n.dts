};

/* eth0	 is connected to a Marvell 88F6560 internal switch, without a PHY. So set
 * fixed speed and duplex.
 */
&eth0 {
	status = "okay";
	phy-mode = "gmii";
	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};
